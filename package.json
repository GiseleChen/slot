{
  "name": "e-game",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve show-room ",
    "build": "ng build show-room ",
    "test": "ng test show-room ",
    "lint": "ng lint show-room ",
    "e2e": "ng e2e",
    "start-lib": "ng build lib-animation --watch",
    "build-lib": "ng build lib-animation",
    "build-fish": "ng build fish-game --prod --aot --buildOptimizer --extractLicenses --no-namedChunks --optimization --no-sourceMap --no-vendorChunk --output-hashing none",
    "build-slot": "ng build slot-game --prod --aot --buildOptimizer --extractLicenses --no-namedChunks --optimization --no-sourceMap --no-vendorChunk --output-hashing none",
    "compile-fish": "cat dist/fish-game/runtime.js dist/fish-game/polyfills.js dist/fish-game/main.js > demo/fish/fish-element.js && cp -R dist/fish-game/assets demo/fish/assets",
    "compile-slot": "cat dist/slot-game/runtime.js dist/slot-game/polyfills.js dist/slot-game/main.js > demo/slot/slot-element.js && cp -R dist/slot-game/assets demo/slot/assets",
    "build-all": "npm run build-lib && npm run build-fish && npm run compile-fish&& npm run build-slot && npm run compile-slot"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.4",
    "@angular/common": "~11.0.4",
    "@angular/compiler": "~11.0.4",
    "@angular/core": "~11.0.4",
    "@angular/elements": "^11.0.4",
    "@angular/forms": "~11.0.4",
    "@angular/platform-browser": "~11.0.4",
    "@angular/platform-browser-dynamic": "~11.0.4",
    "@angular/router": "~11.0.4",
    "document-register-element": "^1.7.2",
    "pixi.js": "^5.3.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "@angular-eslint/builder": "0.8.0-beta.5",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.5",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.5",
    "@angular-eslint/schematics": "^0.8.0-beta.5",
    "@angular-eslint/template-parser": "0.8.0-beta.5",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0"
  }
}
